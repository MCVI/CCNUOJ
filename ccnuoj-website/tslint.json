{
  "extends": "tslint:recommended",
  "globals": {
    "require": true
  },
  "rules": {
    "interface-name": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "object-literal-shorthand": false,
    "whitespace": [
      false,
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator"
    ]
  }
}
